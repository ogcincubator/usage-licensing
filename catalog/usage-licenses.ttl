@prefix cc: <http://creativecommons.org/ns#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ldr: <http://purl.org/NET/ldr/ns#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix usage-action: <https://w3id.org/usage-project/odrl/actions/> .
@prefix usage-license: <https://w3id.org/usage-project/odrl/licenses/> .

usage-license:es-zaragoza-public-license a skos:Concept,
        odrl:Policy ;
    rdfs:label "Licencia de documentos reutilizables del Ayuntamiento de Zaragoza" ;
    dct:source <https://www.zaragoza.es/sede/portal/aviso-legal#condiciones> ;
    skos:broader <https://w3id.org/usage-project/odrl/license> ;
    skos:inScheme <https://w3id.org/usage-project/odrl> ;
    skos:prefLabel "Licencia de documentos reutilizables del Ayuntamiento de Zaragoza" ;
    odrl:duty [ odrl:action cc:Attribution,
                usage-action:preserveOriginalMeaning,
                usage-action:preserveTimestamp ] ;
    odrl:permission [ odrl:action cc:CommercialUse,
                cc:DerivativeWorks,
                cc:Distribution,
                cc:Reproduction,
                cc:ShareAlike,
                ldr:Extraction,
                ldr:Reproduction,
                ldr:Reutilization,
                odrl:extract ] ;
    odrl:prohibition [ odrl:action usage-action:claimResponsibility ] .

usage-license:it-dato-pubblico a skos:Concept,
        odrl:Policy ;
    rdfs:label "Dato pubblico (cfr. art. 1 Codice Amministrazione Digitale Italia)" ;
    dct:source <https://docs.italia.it/italia/piano-triennale-ict/codice-amministrazione-digitale-docs/it/v2018-09-28/_rst/capo6_art68.html> ;
    owl:sameAs <http://purl.org/NET/rdflicense/publicdomain> ;
    skos:broader <https://w3id.org/usage-project/odrl/license> ;
    skos:inScheme <https://w3id.org/usage-project/odrl> ;
    skos:prefLabel "Dato pubblico (cfr. art. 1 Codice Amministrazione Digitale Italia)" .

usage-license:statbel.fgov.be-open-data a skos:Concept,
        odrl:Policy ;
    rdfs:label "StatBel Open Data License" ;
    dct:source <https://statbel.fgov.be/en/open-data> ;
    owl:sameAs <http://purl.org/NET/rdflicense/cc-by4.0> ;
    skos:broader <https://w3id.org/usage-project/odrl/license> ;
    skos:inScheme <https://w3id.org/usage-project/odrl> ;
    skos:prefLabel "StatBel Open Data License" .

usage-license:usgs.public-data a skos:Concept,
        odrl:Policy ;
    rdfs:label "USGS public data" ;
    owl:sameAs <http://purl.org/NET/rdflicense/publicdomain> ;
    skos:broader <https://w3id.org/usage-project/odrl/license> ;
    skos:inScheme <https://w3id.org/usage-project/odrl> ;
    skos:prefLabel "USGS public data" .

usage-action:claimResponsibility a skos:Concept,
        odrl:Action ;
    rdfs:label "Claim responsibility by the original party for any derived or reused data" ;
    skos:broader <https://w3id.org/usage-project/odrl/action> ;
    skos:inScheme <https://w3id.org/usage-project/odrl> ;
    skos:prefLabel "Claim responsibility by the original party for any derived or reused data" .

usage-action:preserveOriginalMeaning a skos:Concept,
        odrl:Action ;
    rdfs:label "Maintain the original meaning of the data" ;
    skos:broader <https://w3id.org/usage-project/odrl/action> ;
    skos:inScheme <https://w3id.org/usage-project/odrl> ;
    skos:prefLabel "Maintain the original meaning of the data" .

usage-action:preserveTimestamp a skos:Concept,
        odrl:Action ;
    rdfs:label "Preserve and communicate latest modification update of the data" ;
    skos:broader <https://w3id.org/usage-project/odrl/action> ;
    skos:inScheme <https://w3id.org/usage-project/odrl> ;
    skos:prefLabel "Preserve and communicate latest modification update of the data" .

<https://w3id.org/usage-project/odrl/action> a skos:Concept ;
    skos:inScheme <https://w3id.org/usage-project/odrl> ;
    skos:narrower usage-action:claimResponsibility,
        usage-action:preserveOriginalMeaning,
        usage-action:preserveTimestamp ;
    skos:prefLabel "USAGE Project ODRL action" ;
    skos:topConceptOf <https://w3id.org/usage-project/odrl> .

<https://w3id.org/usage-project/odrl/license> a skos:Concept ;
    skos:inScheme <https://w3id.org/usage-project/odrl> ;
    skos:narrower usage-license:es-zaragoza-public-license,
        usage-license:it-dato-pubblico,
        usage-license:statbel.fgov.be-open-data,
        usage-license:usgs.public-data ;
    skos:prefLabel "USAGE Project license" ;
    skos:topConceptOf <https://w3id.org/usage-project/odrl> .

<https://w3id.org/usage-project/odrl> a skos:ConceptScheme ;
    skos:hasTopConcept <https://w3id.org/usage-project/odrl/action>,
        <https://w3id.org/usage-project/odrl/license> ;
    skos:prefLabel "USAGE Project ODRL policies and actions" .

